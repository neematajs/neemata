{
  "name": "neematajs",
  "type": "module",
  "scripts": {
    "build": "tsup",
    "type-check": "tsc --noEmit"
  },
  "exports": {
    "./application": {
      "bun": "./src/application.ts",
      "types": "./src/application.ts",
      "default": "./dist/application.js"
    },
    "./server": {
      "bun": "./src/server.ts",
      "types": "./src/server.ts",
      "default": "./dist/server.js"
    },
    "./common": {
      "bun": "./src/common.ts",
      "types": "./src/common.ts",
      "default": "./dist/common.js"
    }
  },
  "dependencies": {
    "@neematajs/common": "workspace:~1.0.0",
    "@neematajs/application": "workspace:~1.0.0",
    "@neematajs/server": "workspace:*",
    "@neematajs/cli": "workspace:*"
  }
}
