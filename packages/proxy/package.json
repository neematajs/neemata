{
  "name": "@nmtjs/proxy",
  "type": "module",
  "scripts": {
    "postinstall": "$npm_execpath build",
    "clean-build": "true",
    "fmt": "cargo fmt",
    "lint": "cargo clippy",
    "build": "$npm_execpath build:debug --release",
    "build:debug": "napi build --esm --js index.js --platform --output-dir ./dist"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "napi": {
    "binaryName": "neemata-proxy"
  },
  "dependencies": {
    "@napi-rs/cli": "^3.4.1"
  },
  "files": [
    "dist",
    "LICENSE.md",
    "README.md"
  ]
}
