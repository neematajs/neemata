{
  "name": "@nmtjs/core",
  "description": "Dependency injection, hooks, and scoped lifecycle management for Neemata.",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.js",
      "module-sync": "./dist/index.js"
    },
    "./constants": {
      "types": "./src/constants.ts",
      "import": "./dist/constants.js",
      "module-sync": "./dist/constants.js"
    }
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "module-sync": "./dist/index.js"
      },
      "./constants": {
        "types": "./dist/constants.d.ts",
        "import": "./dist/constants.js",
        "module-sync": "./dist/constants.js"
      }
    }
  },
  "scripts": {
    "clean-build": "rm -rf ./dist"
  },
  "dependencies": {
    "hookable": "6.0.1"
  },
  "peerDependencies": {
    "@nmtjs/common": "workspace:*",
    "@nmtjs/type": "workspace:*",
    "pino-std-serializers": "^7.0.0",
    "pino": "^10.0.0",
    "pino-pretty": "^13.0.0"
  },
  "devDependencies": {
    "@nmtjs/common": "workspace:*",
    "@nmtjs/type": "workspace:*",
    "pino-std-serializers": "^7.0.0",
    "pino": "^10.0.0",
    "pino-pretty": "^13.0.0",
    "@types/node": "^24"
  },
  "files": [
    "dist",
    "src",
    "LICENSE.md",
    "README.md"
  ]
}
