{
  "name": "@nmtjs/type",
  "type": "module",
  "files": [
    "dist",
    "LICENSE.md",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.js",
      "module-sync": "./dist/index.js"
    },
    "./*": {
      "types": "./src/types/*.ts",
      "import": "./dist/types/*.js",
      "module-sync": "./dist/types/*.js"
    },
    "./temporal": {
      "types": "./src/temporal.ts",
      "import": "./dist/temporal.js",
      "module-sync": "./dist/temporal.js"
    },
    "./temporal-polyfill": {
      "types": "./src/temporal-polyfill.ts",
      "import": "./dist/temporal-polyfill.js",
      "module-sync": "./dist/temporal-polyfill.js"
    }
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "module-sync": "./dist/index.js"
      },
      "./*": {
        "types": "./dist/types/*.d.ts",
        "import": "./dist/types/*.js",
        "module-sync": "./dist/types/*.js"
      },
      "./temporal": {
        "types": "./dist/temporal.d.ts",
        "import": "./dist/temporal.js",
        "module-sync": "./dist/temporal.js"
      },
      "./temporal-polyfill": {
        "types": "./dist/temporal-polyfill.d.ts",
        "import": "./dist/temporal-polyfill.js",
        "module-sync": "./dist/temporal-polyfill.js"
      }
    }
  },
  "scripts": {
    "build": "tsc",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "temporal-spec": "^0.3.0"
  },
  "peerDependencies": {
    "@nmtjs/common": "workspace:*",
    "temporal-polyfill": "^0.3.0",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@nmtjs/common": "workspace:*",
    "temporal-polyfill": "^0.3.0",
    "zod": "catalog:"
  },
  "peerDependenciesMeta": {
    "temporal-polyfill": {
      "optional": true
    }
  }
}
