{
  "name": "@nmtjs/contract",
  "type": "module",
  "exports": {
    ".": {
      "bun": "./src/contract.ts",
      "default": "./dist/contract.js",
      "types": "./src/contract.ts"
    },
    "./compiler": {
      "bun": "./src/compiler.ts",
      "default": "./dist/compiler.js",
      "types": "./src/compiler.ts"
    },
    "./guards": {
      "bun": "./src/guards.ts",
      "default": "./dist/guards.js",
      "types": "./src/guards.ts"
    }
  },
  "scripts": {
    "build": "neemata-build -p neutral --root=./src './**/*.ts'",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "@sinclair/typebox": "^0.32.34",
    "@nmtjs/common": "workspace:*"
  },
  "devDependencies": {
    "@sinclair/typebox": "^0.32.34",
    "@nmtjs/common": "workspace:*"
  },
  "files": [
    "src",
    "dist",
    "tsconfig.json",
    "LICENSE.md",
    "README.md"
  ]
}
