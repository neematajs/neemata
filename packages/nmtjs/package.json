      "module-sync": "./dist/application.js"
    },
    "./server": {
      "types": "./src/server.ts",
      "import": "./dist/server.js",
      "module-sync": "./dist/server.js"
    },
    "./cli": {
      "types": "./src/cli.ts",
      "import": "./dist/cli.js",
      "module-sync": "./dist/cli.js"
    }
