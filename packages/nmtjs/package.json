      "types": "./src/type.ts",
      "import": "./dist/type.js",
      "module-sync": "./dist/type.js"
    },
    "./contract": {
      "types": "./src/contract.ts",
      "import": "./dist/contract.js",
      "module-sync": "./dist/contract.js"
    },
    "./core": {
      "types": "./src/core.ts",
      "import": "./dist/core.js",
      "module-sync": "./dist/core.js"
    },
    "./protocol": {
      "types": "./src/protocol.ts",
      "import": "./dist/protocol.js",
      "module-sync": "./dist/protocol.js"
    },
    "./protocol/client": {
      "types": "./src/protocol-client.ts",
