        "types": "./dist/contract.d.ts",
        "import": "./dist/contract.js",
        "module-sync": "./dist/contract.js"
      },
      "./core": {
        "types": "./dist/core.d.ts",
        "import": "./dist/core.js",
        "module-sync": "./dist/core.js"
      },
      "./protocol": {
        "types": "./dist/protocol.d.ts",
