{
  "name": "nmtjs",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "type-check": "tsc --noEmit"
  },
  "bin": {
    "neemata": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./src/_exports/index.ts",
      "import": "./dist/_exports/index.js",
      "module-sync": "./dist/_exports/index.js"
    },
    "./config": {
      "types": "./src/config.ts",
      "import": "./dist/config.js",
      "module-sync": "./dist/config.js"
    },
    "./runtime": {
      "types": "./src/runtime/*.ts",
      "import": "./dist/runtime/*.js",
      "module-sync": "./dist/runtime/*.js"
    },
    "./*": {
      "types": "./src/_exports/*.ts",
      "import": "./dist/_exports/*.js",
      "module-sync": "./dist/_exports/*.js"
    }
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/_exports/index.d.ts",
        "import": "./dist/_exports/index.js",
        "module-sync": "./dist/_exports/index.js"
      },
      "./config": {
        "types": "./dist/config.d.ts",
        "import": "./dist/config.js",
        "module-sync": "./dist/config.js"
      },
      "./runtime": {
        "types": "./dist/runtime/*.d.ts",
        "import": "./dist/runtime/*.js",
        "module-sync": "./dist/runtime/*.js"
      },
      "./*": {
        "types": "./dist/_exports/*.d.ts",
        "import": "./dist/_exports/*.js",
        "module-sync": "./dist/_exports/*.js"
      }
    }
  },
  "dependencies": {
    "@nmtjs/api": "workspace:*",
    "@nmtjs/application": "workspace:*",
    "@nmtjs/common": "workspace:*",
    "@nmtjs/contract": "workspace:*",
    "@nmtjs/core": "workspace:*",
    "@nmtjs/json-format": "workspace:*",
    "@nmtjs/protocol": "workspace:*",
    "@nmtjs/server": "workspace:*",
    "@nmtjs/type": "workspace:*",
    "@nmtjs/ws-transport": "workspace:*"
  },
  "files": [
    "dist",
    "LICENSE.md",
    "README.md"
  ],
  "devDependencies": {
    "citty": "^0.1.6",
    "rolldown-vite": "^7.2.2"
  }
}
