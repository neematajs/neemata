{
  "name": "nmtjs",
  "type": "module",
  "scripts": {
    "clean-build": "rm -rf ./dist",
    "build": "tsc --declaration --sourcemap",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit"
  },
  "bin": {
    "neemata": "./src/cli.ts"
  },
  "exports": {
    ".": "./src/_exports/index.ts",
    "./config": "./src/config.ts",
    "./*": "./src/_exports/*.ts"
  },
  "publishConfig": {
    "bin": {
      "neemata": "./dist/cli.js"
    },
    "exports": {
      ".": "./dist/_exports/index.js",
      "./config": "./dist/config.js",
      "./*": "./dist/_exports/*.js"
    }
  },
  "dependencies": {
    "@nmtjs/common": "workspace:*",
    "@nmtjs/contract": "workspace:*",
    "@nmtjs/core": "workspace:*",
    "@nmtjs/gateway": "workspace:*",
    "@nmtjs/json-format": "workspace:*",
    "@nmtjs/protocol": "workspace:*",
    "@nmtjs/runtime": "workspace:*",
    "@nmtjs/type": "workspace:*",
    "@nmtjs/ws-transport": "workspace:*",
    "@nmtjs/http-transport": "workspace:*",
    "citty": "^0.1.6",
    "dedent": "^1.7.0",
    "dotenv": "^17.2.3",
    "oxc-resolver": "^11.13.2",
    "vite": "npm:rolldown-vite@7.2.10",
    "vite-plugin-static-copy": "^3.1.4"
  },
  "devDependencies": {
    "@nmtjs/proxy": "workspace:*"
  },
  "peerDependencies": {
    "@nmtjs/proxy": "workspace:*"
  },
  "peerDependenciesMeta": {
    "@nmtjs/proxy": {
      "optional": true
    }
  },
  "files": [
    "dist",
    "LICENSE.md",
    "README.md"
  ]
}
