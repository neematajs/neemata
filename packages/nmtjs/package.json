{
  "name": "nmtjs",
  "description": "Neemata framework CLI and umbrella package.",
  "type": "module",
  "scripts": {
    "clean-build": "rm -rf ./dist"
  },
  "bin": {
    "neemata": "./bin/cli.js"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.js",
      "module-sync": "./dist/index.js"
    },
    "./cli": {
      "types": "./src/cli.ts",
      "import": "./dist/cli.js",
      "module-sync": "./dist/cli.js"
    },
    "./config": {
      "types": "./src/config.ts",
      "import": "./dist/config.js",
      "module-sync": "./dist/config.js"
    },
    "./runtime": {
      "types": "./src/runtime/index.ts",
      "import": "./dist/runtime/index.js",
      "module-sync": "./dist/runtime/index.js"
    },
    "./runtime/types": {
      "types": "./src/runtime/types.ts",
      "import": "./dist/runtime/types.js",
      "module-sync": "./dist/runtime/types.js"
    }
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "module-sync": "./dist/index.js"
      },
      "./cli": {
        "types": "./dist/cli.d.ts",
        "import": "./dist/cli.js",
        "module-sync": "./dist/cli.js"
      },
      "./config": {
        "types": "./dist/config.d.ts",
        "import": "./dist/config.js",
        "module-sync": "./dist/config.js"
      },
      "./runtime": {
        "types": "./dist/runtime/index.d.ts",
        "import": "./dist/runtime/index.js",
        "module-sync": "./dist/runtime/index.js"
      },
      "./runtime/types": {
        "types": "./dist/runtime/types.d.ts",
        "import": "./dist/runtime/types.js",
        "module-sync": "./dist/runtime/types.js"
      }
    }
  },
  "dependencies": {
    "@bull-board/api": "6.16.4",
    "@bull-board/h3": "6.16.4",
    "@bull-board/ui": "6.16.4",
    "@nmtjs/common": "workspace:*",
    "@nmtjs/contract": "workspace:*",
    "@nmtjs/core": "workspace:*",
    "@nmtjs/gateway": "workspace:*",
    "@nmtjs/http-transport": "workspace:*",
    "@nmtjs/json-format": "workspace:*",
    "@nmtjs/msgpack-format": "workspace:*",
    "@nmtjs/protocol": "workspace:*",
    "@nmtjs/type": "workspace:*",
    "@nmtjs/ws-transport": "workspace:*",
    "bullmq": "5.67.2",
    "citty": "0.2.0",
    "dedent": "^1.7.0",
    "dotenv": "^17.2.3",
    "ejs": "3.1.10",
    "h3": "1.15.4",
    "ioredis": "^5.9.2",
    "iovalkey": "^0.3.3",
    "oxc-resolver": "11.17.0",
    "prom-client": "git@github.com:siimon/prom-client.git#d4d2dcb366384833951e0116caca707b5f62aa5e",
    "vite": "catalog:"
  },
  "devDependencies": {
    "@nmtjs/proxy": "catalog:",
    "pino": "10.3.0",
    "zod": "catalog:"
  },
  "peerDependencies": {
    "@nmtjs/proxy": "catalog:",
    "zod": "catalog:"
  },
  "peerDependenciesMeta": {
    "@nmtjs/proxy": {
      "optional": true
    }
  },
  "files": [
    "bin",
    "dist",
    "src",
    "LICENSE.md",
    "README.md"
  ]
}
