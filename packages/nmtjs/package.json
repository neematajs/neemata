{
  "name": "nmtjs",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "type-check": "tsc --noEmit"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.js",
      "module-sync": "./dist/index.js"
    },
    "./common": {
      "types": "./src/common.ts",
      "import": "./dist/common.js",
      "module-sync": "./dist/common.js"
    },
