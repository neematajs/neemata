{
  "name": "nmtjs",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "type-check": "tsc --noEmit"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.js",
      "module-sync": "./dist/index.js"
    },
    "./*": {
      "types": "./src/*.ts",
      "import": "./dist/*.js",
      "module-sync": "./dist/*.js"
    }
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "module-sync": "./dist/index.js"
      },
      "./*": {
        "types": "./dist/*.d.ts",
        "import": "./dist/*.js",
        "module-sync": "./dist/*.js"
      }
    }
  },
  "dependencies": {
    "@nmtjs/application": "workspace:*",
    "@nmtjs/cli": "workspace:*",
    "@nmtjs/common": "workspace:*",
    "@nmtjs/contract": "workspace:*",
    "@nmtjs/core": "workspace:*",
    "@nmtjs/json-format": "workspace:*",
    "@nmtjs/protocol": "workspace:*",
    "@nmtjs/server": "workspace:*",
    "@nmtjs/type": "workspace:*",
    "@nmtjs/ws-transport": "workspace:*"
  },
  "files": [
    "dist",
    "LICENSE.md",
    "README.md"
  ]
}
