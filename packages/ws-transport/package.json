{
  "name": "@nmtjs/ws-transport",
  "description": "WebSocket server transport for Neemata runtimes.",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.js",
      "module-sync": "./dist/index.js"
    },
    "./bun": {
      "types": "./src/runtimes/bun.ts",
      "import": "./dist/runtimes/bun.js",
      "module-sync": "./dist/runtimes/bun.js"
    },
    "./deno": {
      "types": "./src/runtimes/deno.ts",
      "import": "./dist/runtimes/deno.js",
      "module-sync": "./dist/runtimes/deno.js"
    },
    "./node": {
      "types": "./src/runtimes/node.ts",
      "import": "./dist/runtimes/node.js",
      "module-sync": "./dist/runtimes/node.js"
    }
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "module-sync": "./dist/index.js"
      },
      "./bun": {
        "types": "./dist/runtimes/bun.d.ts",
        "import": "./dist/runtimes/bun.js",
        "module-sync": "./dist/runtimes/bun.js"
      },
      "./deno": {
        "types": "./dist/runtimes/deno.d.ts",
        "import": "./dist/runtimes/deno.js",
        "module-sync": "./dist/runtimes/deno.js"
      },
      "./node": {
        "types": "./dist/runtimes/node.d.ts",
        "import": "./dist/runtimes/node.js",
        "module-sync": "./dist/runtimes/node.js"
      }
    }
  },
  "scripts": {
    "clean-build": "rm -rf ./dist"
  },
  "dependencies": {
    "crossws": "0.4.4"
  },
  "devDependencies": {
    "@nmtjs/client": "workspace:*",
    "@nmtjs/common": "workspace:*",
    "@nmtjs/gateway": "workspace:*",
    "@nmtjs/protocol": "workspace:*",
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#v20.57.0"
  },
  "peerDependencies": {
    "@nmtjs/common": "workspace:*",
    "@nmtjs/core": "workspace:*",
    "@nmtjs/gateway": "workspace:*",
    "@nmtjs/protocol": "workspace:*",
    "@types/bun": "^1.3.0",
    "@types/deno": "^2.3.0",
    "@types/node": "^24",
    "uWebSockets.js": "^20.57.0"
  },
  "peerDependenciesMeta": {
    "@types/bun": {
      "optional": true
    },
    "@types/deno": {
      "optional": true
    },
    "@types/node": {
      "optional": true
    },
    "uWebSockets.js": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=24",
    "bun": ">=1.3.0",
    "deno": ">=2.3.0"
  },
  "files": [
    "dist",
    "src",
    "LICENSE.md",
    "README.md"
  ]
}
