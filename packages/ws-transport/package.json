{
  "name": "@nmtjs/ws-transport",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.js",
      "module-sync": "./dist/index.js"
    },
    "./bun": {
      "types": "./src/runtimes/bun.ts",
      "import": "./dist/runtimes/bun.js"
    },
    "./deno": {
      "types": "./src/runtimes/deno.ts",
      "import": "./dist/runtimes/deno.js"
    },
    "./node": {
      "types": "./src/runtimes/node.ts",
      "import": "./dist/runtimes/node.js"
    }
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "module-sync": "./dist/index.js"
      },
      "./bun": "./dist/runtimes/bun.js",
      "./deno": "./dist/runtimes/deno.js",
      "./node": "./dist/runtimes/node.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "crossws": "0.4.1"
  },
  "devDependencies": {
    "@nmtjs/client": "workspace:*",
    "@nmtjs/common": "workspace:*",
    "@nmtjs/protocol": "workspace:*",
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#v20.52.0"
  },
  "peerDependencies": {
    "@nmtjs/common": "workspace:*",
    "@nmtjs/core": "workspace:*",
    "@nmtjs/protocol": "workspace:*",
    "@types/bun": "^1.2.19",
    "@types/deno": "^2.3.0",
    "@types/node": "^20",
    "uWebSockets.js": "^20.52.0"
  },
  "peerDependenciesMeta": {
    "@types/bun": {
      "optional": true
    },
    "@types/deno": {
      "optional": true
    },
    "@types/node": {
      "optional": true
    },
    "uWebSockets.js": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=20",
    "bun": ">=1.2.19",
    "deno": ">=2.3.0"
  },
  "files": [
    "dist",
    "LICENSE.md",
    "README.md"
  ]
}
