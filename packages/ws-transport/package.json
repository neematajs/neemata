{
  "name": "@nmtjs/ws-transport",
  "description": "WebSocket server transport for Neemata runtimes.",
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./bun": "./src/runtimes/bun.ts",
    "./deno": "./src/runtimes/deno.ts",
    "./node": "./src/runtimes/node.ts"
  },
  "publishConfig": {
    "exports": {
      ".": "./dist/index.js",
      "./bun": "./dist/runtimes/bun.js",
      "./deno": "./dist/runtimes/deno.js",
      "./node": "./dist/runtimes/node.js"
    }
  },
  "scripts": {
    "clean-build": "rm -rf ./dist"
  },
  "dependencies": {
    "crossws": "0.4.4"
  },
  "devDependencies": {
    "@nmtjs/client": "workspace:*",
    "@nmtjs/common": "workspace:*",
    "@nmtjs/gateway": "workspace:*",
    "@nmtjs/protocol": "workspace:*",
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#v20.57.0"
  },
  "peerDependencies": {
    "@nmtjs/common": "workspace:*",
    "@nmtjs/core": "workspace:*",
    "@nmtjs/gateway": "workspace:*",
    "@nmtjs/protocol": "workspace:*",
    "@types/bun": "^1.3.0",
    "@types/deno": "^2.3.0",
    "@types/node": "^24",
    "uWebSockets.js": "^20.57.0"
  },
  "peerDependenciesMeta": {
    "@types/bun": {
      "optional": true
    },
    "@types/deno": {
      "optional": true
    },
    "@types/node": {
      "optional": true
    },
    "uWebSockets.js": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=24",
    "bun": ">=1.3.0",
    "deno": ">=2.3.0"
  },
  "files": [
    "dist",
    "src",
    "LICENSE.md",
    "README.md"
  ]
}
