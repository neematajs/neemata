{
  "name": "@nmtjs/server",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "type-check": "tsc --noEmit"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.js",
      "module-sync": "./dist/index.js"
    }
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "module-sync": "./dist/index.js"
      }
    }
  },
  "dependencies": {
    "@nmtjs/application": "workspace:*",
    "@nmtjs/common": "workspace:*",
    "@nmtjs/core": "workspace:*"
  },
  "peerDependencies": {
    "@nmtjs/application": "workspace:*",
    "@nmtjs/common": "workspace:*",
    "@nmtjs/core": "workspace:*"
  },
  "files": [
    "src",
    "dist",
    "LICENSE.md",
    "README.md"
  ]
}
