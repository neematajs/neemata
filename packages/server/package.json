{
  "name": "@nmtjs/server",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "type-check": "tsc --noEmit"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.js",
      "module-sync": "./dist/index.js"
    },
    "./worker": {
      "types": "./src/worker.ts",
      "import": "./dist/worker.js",
      "module-sync": "./dist/worker.js"
    }
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "module-sync": "./dist/index.js"
      },
      "./worker": {
        "types": "./dist/worker.ts",
        "import": "./dist/worker.js",
        "module-sync": "./dist/worker.js"
      }
    }
  },
  "devDependencies": {
    "@nmtjs/api": "workspace:*",
    "@nmtjs/application": "workspace:*",
    "@nmtjs/common": "workspace:*",
    "@nmtjs/core": "workspace:*",
    "@nmtjs/json-format": "workspace:^",
    "@nmtjs/protocol": "workspace:^",
    "@nmtjs/type": "workspace:*",
    "bullmq": "5.63.0",
    "ioredis": "^5.8.2",
    "zod": "catalog:"
  },
  "peerDependencies": {
    "@nmtjs/api": "workspace:*",
    "@nmtjs/application": "workspace:*",
    "@nmtjs/common": "workspace:*",
    "@nmtjs/core": "workspace:*",
    "@nmtjs/json-format": "workspace:^",
    "@nmtjs/protocol": "workspace:^",
    "@nmtjs/type": "workspace:*",
    "bullmq": "5.63.0",
    "ioredis": "^5.8.2",
    "zod": "catalog:"
  },
  "files": [
    "dist",
    "LICENSE.md",
    "README.md"
  ],
  "dependencies": {
    "piscina": "^5.1.3"
  }
}
