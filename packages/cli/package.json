{
  "name": "@nmtjs/cli",
  "type": "module",
  "bin": {
    "neemata": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "default": "./dist/run.js",
      "types": "./src/run.ts"
    }
  },
  "scripts": {
    "build": "neemata-build --root=./src './**/*.ts'",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "tsx": "^4.10.5"
  },
  "peerDependencies": {
    "@nmtjs/application": "workspace:*",
    "@nmtjs/server": "workspace:*"
  },
  "devDependencies": {
    "@nmtjs/application": "workspace:*",
    "@nmtjs/server": "workspace:*"
  },
  "files": [
    "src",
    "dist",
    "tsconfig.json",
    "LICENSE.md",
    "README.md"
  ]
