{
  "name": "@nmtjs/gateway",
  "description": "Connection management and RPC dispatching gateway for Neemata.",
  "type": "module",
  "scripts": {
    "clean-build": "rm -rf ./dist"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.js",
      "module-sync": "./dist/index.js"
    }
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "module-sync": "./dist/index.js"
      }
    }
  },
  "dependencies": {
    "@nmtjs/common": "workspace:*",
    "@nmtjs/core": "workspace:*",
    "@nmtjs/type": "workspace:*",
    "@nmtjs/protocol": "workspace:*",
    "hookable": "6.0.0-rc.1"
  },
  "peerDependencies": {
    "@nmtjs/common": "workspace:*",
    "@nmtjs/core": "workspace:*",
    "@nmtjs/type": "workspace:*",
    "@nmtjs/protocol": "workspace:*"
  },
  "devDependencies": {
    "@nmtjs/_tests": "workspace:*"
  },
  "files": [
    "dist",
    "src",
    "LICENSE.md",
    "README.md"
  ]
}
