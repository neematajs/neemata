{
  "name": "@nmtjs/runtime",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "type-check": "tsc --noEmit"
  },
  "exports": {
    ".": "./src/index.ts",
    "./types": "./src/types.ts",
    "./worker": "./src/worker.ts"
