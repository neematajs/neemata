{
  "name": "@nmtjs/runtime",
  "type": "module",
  "scripts": {
    "clean-build": "tsc --build --clean",
    "build": "tsc --build --declaration --sourceMap",
    "type-check": "tsc --noEmit"
  },
  "exports": {
    ".": "./src/index.ts",
    "./application": "./src/application/index.ts",
    "./worker": "./src/worker.ts",
    "./types": "./src/types.ts"
  },
  "publishConfig": {
    "exports": {
      ".": "./dist/index.js",
      "./application": "./dist/application.js",
      "./worker": "./dist/worker.js",
      "./types": "./dist/types.js"
    }
  },
  "dependencies": {
    "vite": "npm:rolldown-vite@^7.2.6",
    "vite-plugin-static-copy": "^3.1.4",
    "citty": "0.1.6"
  },
  "devDependencies": {
    "@nmtjs/common": "workspace:*",
    "@nmtjs/core": "workspace:*",
    "@nmtjs/gateway": "workspace:*",
    "@nmtjs/json-format": "workspace:*",
    "@nmtjs/protocol": "workspace:*",
    "@nmtjs/proxy": "workspace:*",
    "@nmtjs/type": "workspace:*",
    "bullmq": "5.63.0",
    "ioredis": "^5.8.2",
    "iovalkey": "^0.3.3",
    "zod": "catalog:"
  },
  "peerDependencies": {
    "@nmtjs/common": "workspace:*",
    "@nmtjs/core": "workspace:*",
    "@nmtjs/gateway": "workspace:*",
    "@nmtjs/json-format": "workspace:*",
    "@nmtjs/protocol": "workspace:*",
    "@nmtjs/proxy": "workspace:*",
    "@nmtjs/type": "workspace:*",
    "bullmq": "5.63.0",
    "ioredis": "^5.8.2",
    "iovalkey": "^0.3.3",
    "zod": "catalog:"
  },
  "peerDependenciesMeta": {
    "@nmtjs/proxy": {
      "optional": true
    }
  },
  "files": [
    "dist",
    "LICENSE.md",
    "README.md"
  ]
}
