{
  "name": "@nmtjs/client",
  "type": "module",
  "scripts": {
    "clean-build": "tsc --build --clean",
    "build": "tsc --build --declaration --sourceMap",
    "type-check": "tsc --noEmit"
  },
  "exports": {
    ".": "./src/index.ts",
    "./runtime": "./src/clients/runtime.ts",
    "./static": "./src/clients/static.ts"
  },
  "publishConfig": {
    "exports": {
      ".": "./dist/index.js",
      "./runtime": "./dist/clients/runtime.js",
      "./static": "./dist/clients/static.js"
    }
  },
  "peerDependencies": {
    "@nmtjs/type": "workspace:*",
    "@nmtjs/contract": "workspace:*",
    "@nmtjs/common": "workspace:*",
    "@nmtjs/protocol": "workspace:*"
  },
  "devDependencies": {
    "@nmtjs/type": "workspace:*",
    "@nmtjs/contract": "workspace:*",
    "@nmtjs/common": "workspace:*",
    "@nmtjs/protocol": "workspace:*"
  },
  "files": [
    "dist",
    "LICENSE.md",
    "README.md"
  ]
}
