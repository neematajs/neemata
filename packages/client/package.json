{
  "name": "@nmtjs/client",
  "description": "Base client infrastructure for Neemata RPC calls.",
  "type": "module",
  "scripts": {
    "clean-build": "rm -rf ./dist"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.js",
      "module-sync": "./dist/index.js"
    },
    "./runtime": {
      "types": "./src/clients/runtime.ts",
      "import": "./dist/clients/runtime.js",
      "module-sync": "./dist/clients/runtime.js"
    },
    "./static": {
      "types": "./src/clients/static.ts",
      "import": "./dist/clients/static.js",
      "module-sync": "./dist/clients/static.js"
    }
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "module-sync": "./dist/index.js"
      },
      "./runtime": {
        "types": "./dist/clients/runtime.d.ts",
        "import": "./dist/clients/runtime.js",
        "module-sync": "./dist/clients/runtime.js"
      },
      "./static": {
        "types": "./dist/clients/static.d.ts",
        "import": "./dist/clients/static.js",
        "module-sync": "./dist/clients/static.js"
      }
    }
  },
  "peerDependencies": {
    "@nmtjs/type": "workspace:*",
    "@nmtjs/contract": "workspace:*",
    "@nmtjs/common": "workspace:*",
    "@nmtjs/protocol": "workspace:*"
  },
  "devDependencies": {
    "@nmtjs/_tests": "workspace:*",
    "@nmtjs/type": "workspace:*",
    "@nmtjs/contract": "workspace:*",
    "@nmtjs/common": "workspace:*",
    "@nmtjs/protocol": "workspace:*"
  },
  "files": [
    "dist",
    "src",
    "LICENSE.md",
    "README.md"
  ]
}
