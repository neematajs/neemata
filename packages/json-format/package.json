{
  "name": "@neematajs/json-format",
  "type": "module",
  "exports": {
    "./client": {
      "bun": "./src/client.ts",
      "default": "./dist/client.js",
      "types": "./src/client.ts"
    },
    "./server": {
      "bun": "./src/server.ts",
      "default": "./dist/server.js",
      "types": "./src/server.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "@neematajs/common": "workspace:*",
    "@neematajs/application": "workspace:*"
  },
  "devDependencies": {
    "@neematajs/common": "workspace:*",
    "@neematajs/application": "workspace:*"
  }
}